{
  "name": "export-type",
  "version": "0.0.1",
  "description": "从 javascript 字面量导出 typescript 类型",
  "main": "./dist/main.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "dev": "rollup -c  --watch",
    "watch": "tsc -w",
    "test": "jest",
    "test:q": "jest --collectCoverage false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/any86/export-type.git"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.genTsType",
        "title": "⚡ generate typescript type (生成ts类型)"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "typescript",
    "cli",
    "type",
    "declare"
  ],
  "author": "any86",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/any86/export-type/issues"
  },
  "homepage": "https://github.com/any86/export-type#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.4.0",
    "@types/vscode": "^1.63.1",
    "jest": "^27.4.7",
    "json5": "^2.2.0",
    "rollup": "^2.64.0",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}